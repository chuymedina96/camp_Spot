<h1>This is the campgrounds page</h1>

<% campgrounds.forEach(function(campground){ %>
    <div class="container">
        <div class="row">
            <div class = "col-lg-12 text-center">
                <h3><%= campground.name %></h3>
                <img src="<%= campground.image %>"></img>
            </div>
        </div>
    </div>

<% }); %>
